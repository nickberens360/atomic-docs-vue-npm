{
  "name": "component-documentation",
  "version": "1.0.1",
  "description": "A Vue 3 plugin to automatically generate documentation for your components, providing an interactive way to view examples and props.",
  "main": "dist/component-documentation.umd.js",
  "module": "dist/component-documentation.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/component-documentation.es.js",
      "require": "./dist/component-documentation.umd.js"
    },
    "./routes": {
      "types": "./dist/routes.d.ts",
      "import": "./dist/routes.es.js",
      "require": "./dist/routes.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickberens360/atomic-docs-vue-npm.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "documentation",
    "docs",
    "generator",
    "vite",
    "plugin"
  ],
  "author": "Nick Berens",
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "vue": "^3.2.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^20.14.13",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "canvas": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.4.0",
    "vite": "^5.3.5",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.0.29"
  },
  "dependencies": {
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  }
}
