{
  "name": "vue-atomic-docs",
  "version": "2.1.4",
  "description": "A Vue plugin to automatically generate documentation for your components.",
  "author": "Nick Berens nickberens360@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickberens360/atomic-docs-vue-npm.git"
  },
  "main": "dist/vue-atomic-docs.umd.js",
  "module": "dist/vue-atomic-docs.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "src/routes.ts",
    "src/types.ts",
    "src/components"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declarationDir dist --project tsconfig.build.json --rootDir .",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build",
    "release": "npm run release:patch",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "release:next": "npm version prepatch --preid=next && npm publish --tag next",
    "postrelease": "git push && git push --tags"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.0.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "@vue/compiler-sfc": "^3.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "canvas": "^3.1.0",
    "jsdom": "^26.1.0",
    "path": "^0.12.7",
    "sass": "^1.89.2",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vitest": "^3.1.4",
    "vue": "^3.4.0",
    "vue-router": "^4.3.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "plugin",
    "documentation",
    "components",
    "docs",
    "atomic",
    "vite"
  ],
  "private": false
}
