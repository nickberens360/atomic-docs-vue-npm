{
  "name": "vue-component-docs-plugin",
  "version": "1.0.0",
  "description": "A Vue plugin to automatically generate documentation for your components.",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickberens360/atomic-docs-vue-npm.git"
  },
  "main": "dist/vue-component-docs-plugin.umd.js",
  "module": "dist/vue-component-docs-plugin.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "src/vite-raw-sources-plugin.ts",
    "src/shims-virtual.d.ts",
    "src/routes.ts",
    "src/types.ts",
    "src/components"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declarationDir dist --project tsconfig.build.json",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "prismjs": "^1.30.0",
    "glob": "^10.0.0"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "@vue/compiler-sfc": "^3.4.0",
    "@vue/test-utils": "^2.4.6",
    "canvas": "^3.1.0",
    "jsdom": "^26.1.0",
    "path": "^0.12.7",
    "@types/glob": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.2.0",
    "vite-plugin-css-injected-by-js": "^3.5.0",
    "vitest": "^3.1.4",
    "vue": "^3.4.0",
    "vue-router": "^4.3.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "plugin",
    "documentation",
    "components",
    "docs",
    "atomic",
    "vite"
  ],
  "private": false
}